<h1>诱捕会话</h1>
<p><a class="btn" href="/aaadmin">返回</a></p>
<div class="table-wrap">
  <table>
    <thead>
      <tr>
        <th>会话ID</th>
        <th>创建时间</th>
        <th>首次IP</th>
        <th>UA</th>
        <th>最近活动</th>
        <th>事件数</th>
      </tr>
    </thead>
    <tbody>
      <% rows.forEach(r => { %>
        <tr>
          <td class="mono small"><%= r.session_id %></td>
          <td><%= r.created_at %></td>
          <td><%= r.first_ip %></td>
          <td class="small"><%= r.user_agent %></td>
          <td><%= r.last_ts || '-' %></td>
          <td><%= r.event_count %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  
</div>
